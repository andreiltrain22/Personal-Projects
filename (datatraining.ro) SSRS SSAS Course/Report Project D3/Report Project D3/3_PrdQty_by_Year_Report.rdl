  <DataSets>
    <DataSet Name="MainDS">
      <Query>
        <DataSourceName>TrainingDB</DataSourceName>
        <CommandText>SELECT        Production.Products.productname, YEAR(Sales.Orders.orderdate) AS OrderYear, SUM(Sales.OrderDetails.qty) AS TotalQty
FROM            Sales.OrderDetails INNER JOIN
                         Production.Products ON Sales.OrderDetails.productid = Production.Products.productid INNER JOIN
                         Sales.Orders ON Sales.OrderDetails.orderid = Sales.Orders.orderid
GROUP BY Production.Products.productname, YEAR(Sales.Orders.orderdate)</CommandText>
      </Query>
      <Fields>
        <Field Name="productname">
          <DataField>productname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderYear">
          <DataField>OrderYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalQty">
          <DataField>TotalQty</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
