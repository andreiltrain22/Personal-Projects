  <DataSets>
    <DataSet Name="DataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        Sales.Orders.shipcountry, YEAR(Sales.Orders.orderdate) AS OrderYear, SUM(Sales.OrderDetails.unitprice * Sales.OrderDetails.qty) AS SalesAmt
FROM            Sales.OrderDetails INNER JOIN
                         Sales.Orders ON Sales.OrderDetails.orderid = Sales.Orders.orderid
GROUP BY Sales.Orders.shipcountry, YEAR(Sales.Orders.orderdate)</CommandText>
      </Query>
      <Fields>
        <Field Name="shipcountry">
          <DataField>shipcountry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderYear">
          <DataField>OrderYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SalesAmt">
          <DataField>SalesAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!shipcountry.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!p_ShipCtry.Value()</FilterValue>
          </FilterValues>
        </Filter>
        <Filter>
          <FilterExpression>=Fields!OrderYear.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!p_Yr.Value(0)</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
