  <DataSets>
    <DataSet Name="MainDataSet">
      <Query>
        <DataSourceName>TrainingDB</DataSourceName>
        <CommandText>SELECT        HR.Employees.firstname + ' ' + HR.Employees.lastname AS EmployeeName, SUM(Sales.OrderDetails.unitprice * Sales.OrderDetails.qty) AS SalesAmt
FROM            HR.Employees INNER JOIN
                         Sales.Orders ON HR.Employees.empid = Sales.Orders.empid INNER JOIN
                         Sales.OrderDetails ON Sales.Orders.orderid = Sales.OrderDetails.orderid
GROUP BY HR.Employees.firstname + ' ' + HR.Employees.lastname</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeName">
          <DataField>EmployeeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesAmt">
          <DataField>SalesAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
