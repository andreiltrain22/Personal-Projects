  <DataSets>
    <DataSet Name="MainDataSet">
      <Query>
        <DataSourceName>TrainingDB</DataSourceName>
        <CommandText>SELECT        Sales.Customers.companyname, Production.Categories.categoryname, Sales.Orders.shipcountry, SUM(Sales.OrderDetails.unitprice * Sales.OrderDetails.qty) 
                         AS SalesAmt
FROM            Sales.Orders INNER JOIN
                         Sales.Customers ON Sales.Orders.custid = Sales.Customers.custid INNER JOIN
                         Sales.OrderDetails ON Sales.Orders.orderid = Sales.OrderDetails.orderid INNER JOIN
                         Production.Products ON Sales.OrderDetails.productid = Production.Products.productid INNER JOIN
                         Production.Categories ON Production.Products.categoryid = Production.Categories.categoryid AND 
                         Production.Products.categoryid = Production.Categories.categoryid AND Production.Products.categoryid = Production.Categories.categoryid
GROUP BY Sales.Customers.companyname, Production.Categories.categoryname, Sales.Orders.shipcountry</CommandText>
      </Query>
      <Fields>
        <Field Name="companyname">
          <DataField>companyname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="categoryname">
          <DataField>categoryname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="shipcountry">
          <DataField>shipcountry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesAmt">
          <DataField>SalesAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
