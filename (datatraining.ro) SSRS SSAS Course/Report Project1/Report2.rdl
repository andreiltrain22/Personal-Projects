  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>training</DataSourceName>
        <CommandText>select e.firstname + ' ' + e.lastname as fullname, e.title, e.hiredate, e.address, o.orderdate
from hr.Employees e
inner join sales.orders o on o.empid = e.empid</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
