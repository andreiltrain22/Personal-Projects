  <DataSets>
    <DataSet Name="DataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        Sales.Orders.shipcity, Sales.Orders.shipcountry, SUM(Sales.OrderDetails.unitprice * Sales.OrderDetails.qty) AS SalesAmt
FROM            Sales.Orders INNER JOIN
                         Sales.OrderDetails ON Sales.Orders.orderid = Sales.OrderDetails.orderid
GROUP BY Sales.Orders.shipcity, Sales.Orders.shipcountry</CommandText>
      </Query>
      <Fields>
        <Field Name="shipcity">
          <DataField>shipcity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="shipcountry">
          <DataField>shipcountry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesAmt">
          <DataField>SalesAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!shipcountry.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!p_Country.Value()</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="Param_DataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>select distinct shipcountry from sales.orders</CommandText>
      </Query>
      <Fields>
        <Field Name="shipcountry">
          <DataField>shipcountry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
