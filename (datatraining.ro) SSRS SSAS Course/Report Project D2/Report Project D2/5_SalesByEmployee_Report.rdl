  <DataSets>
    <DataSet Name="DataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        HR.Employees.firstname + ' ' + HR.Employees.lastname AS EmployeeName, YEAR(Sales.Orders.orderdate) AS OrderYear, sum(Sales.OrderDetails.qty) AS Qty, 
                         sum(Sales.OrderDetails.qty * Sales.OrderDetails.unitprice) AS SalesAmt
FROM            HR.Employees INNER JOIN
                         Sales.Orders ON HR.Employees.empid = Sales.Orders.empid INNER JOIN
                         Sales.OrderDetails ON Sales.Orders.orderid = Sales.OrderDetails.orderid
GROUP BY HR.Employees.firstname + ' ' + HR.Employees.lastname, YEAR(Sales.Orders.orderdate)</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeName">
          <DataField>EmployeeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderYear">
          <DataField>OrderYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Qty">
          <DataField>Qty</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SalesAmt">
          <DataField>SalesAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!OrderYear.Value</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=Parameters!p_OrderYear.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="ParamDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT DISTINCT YEAR(orderdate) as OrderYear
FROM            sales.orders</CommandText>
      </Query>
      <Fields>
        <Field Name="OrderYear">
          <DataField>OrderYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
