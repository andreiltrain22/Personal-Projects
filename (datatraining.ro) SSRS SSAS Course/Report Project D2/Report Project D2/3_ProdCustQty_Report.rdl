  <DataSets>
    <DataSet Name="DataSetProdCustQty">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        Production.Products.productname, Sales.Customers.companyname, SUM(Sales.OrderDetails.qty * Sales.OrderDetails.unitprice) AS SalesAmt
FROM            Production.Products INNER JOIN
                         Sales.OrderDetails ON Production.Products.productid = Sales.OrderDetails.productid INNER JOIN
                         Sales.Orders ON Sales.OrderDetails.orderid = Sales.Orders.orderid INNER JOIN
                         Sales.Customers ON Sales.Orders.custid = Sales.Customers.custid
GROUP BY Production.Products.productname, Sales.Customers.companyname</CommandText>
      </Query>
      <Fields>
        <Field Name="productname">
          <DataField>productname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="companyname">
          <DataField>companyname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesAmt">
          <DataField>SalesAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!companyname.Value</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=Parameters!p_CustName.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="ParamDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>select distinct companyname from sales.customers</CommandText>
      </Query>
      <Fields>
        <Field Name="companyname">
          <DataField>companyname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
