  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Training</DataSourceName>
        <CommandText>SELECT        HR.Employees.country, HR.Employees.title, SUM(Sales.OrderDetails.unitprice * Sales.OrderDetails.qty) AS SalesAmt
FROM            HR.Employees INNER JOIN
                         Sales.Orders ON HR.Employees.empid = Sales.Orders.empid INNER JOIN
                         Sales.OrderDetails ON Sales.Orders.orderid = Sales.OrderDetails.orderid
GROUP BY HR.Employees.country, HR.Employees.title</CommandText>
      </Query>
      <Fields>
        <Field Name="country">
          <DataField>country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="title">
          <DataField>title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesAmt">
          <DataField>SalesAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
