  <DataSets>
    <DataSet Name="DataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        YEAR(Sales.Orders.orderdate) AS OrderYear, Sales.Customers.country, SUM(Sales.OrderDetails.unitprice * Sales.OrderDetails.qty) AS SalesAmt
FROM            Sales.Customers INNER JOIN
                         Sales.Orders ON Sales.Customers.custid = Sales.Orders.custid INNER JOIN
                         Sales.OrderDetails ON Sales.Orders.orderid = Sales.OrderDetails.orderid
GROUP BY YEAR(Sales.Orders.orderdate), Sales.Customers.country</CommandText>
      </Query>
      <Fields>
        <Field Name="OrderYear">
          <DataField>OrderYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="country">
          <DataField>country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesAmt">
          <DataField>SalesAmt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!country.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!p_Country.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="Ctry_DataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>select distinct country from sales.customers</CommandText>
      </Query>
      <Fields>
        <Field Name="country">
          <DataField>country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
